<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.te37"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="android.te38.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="android.te38.SecondActivity"
            android:label="@string/title_activity_second" >
            <intent-filter>
                <action android:name="android.te38.SA" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <!-- name定义了一个别名SA 别名可以用任何名字 -->
        </activity>
        <activity
            android:name="android.te37.ThirdActivity"
            android:label="@string/title_activity_third" >
        </activity>
        <activity
            android:name="android.te37.EmmmActivity"
            android:label="@string/title_activity_emmm" >
        </activity>
        <activity
            android:name="android.te37.UhhhActivity"
            android:label="@string/title_activity_uhhh" >
        </activity>
    </application>

</manifest>
